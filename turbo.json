{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DATABASE_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "DODO_API_KEY",
    "DODO_WEBHOOK_SECRET",
    "DODO_PRO_PRODUCT_ID",
    "RESEND_API_KEY",
    "NEXT_PUBLIC_APP_URL",
    "NODE_ENV"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "out/**",
        ".next/**",
        "!.next/cache/**",
        ".output/**"
      ]
    },
    "build:mac": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**"],
      "cache": false
    },
    "build:win": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**"],
      "cache": false
    },
    "build:linux": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "typecheck:node": {
      "outputs": []
    },
    "typecheck:web": {
      "outputs": []
    },
    "types:check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [],
      "cache": false
    }
  }
}
